{
  "compilerOptions": {
    "target": "es5",
    "module": "es2020",
    "moduleResolution": "Node",
    "sourceMap": true,
    "isolatedModules": true,
    "strict": true, 
    "alwaysStrict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
